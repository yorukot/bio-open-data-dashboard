    "/api/tbia-data": {
      "get": {
        "summary": "Get TBIA biodiversity data",
        "description": "Retrieve Taiwan Biodiversity Information (TBIA) dataset with multiple filtering options",
        "tags": ["TBIA Data"],
        "parameters": [
          {
            "name": "start_time",
            "in": "query",
            "required": false,
            "description": "Start time for data retrieval (defaults to 2020-01-01T00:00:00Z)",
            "schema": {
              "type": "string",
              "format": "date-time",
              "default": "2020-01-01T00:00:00Z",
              "example": "2023-01-01T00:00:00Z"
            }
          },
          {
            "name": "end_time",
            "in": "query",
            "required": false,
            "description": "End time for data retrieval (defaults to current date)",
            "schema": {
              "type": "string",
              "format": "date-time",
              "example": "2023-12-31T23:59:59Z"
            }
          },
          {
            "name": "bio_group",
            "in": "query",
            "required": false,
            "description": "Filter by biological group (Chinese values)",
            "schema": {
              "type": "string",
              "enum": ["鳥類", "兩棲類", "哺乳類", "爬蟲類", "魚類", "昆蟲", "蜘蛛"],
              "example": "鳥類"
            }
          },
          {
            "name": "common_name_c",
            "in": "query",
            "required": false,
            "description": "Filter by common name (partial match, case-insensitive)",
            "schema": {
              "type": "string",
              "example": "臺灣"
            }
          },
          {
            "name": "county",
            "in": "query",
            "required": false,
            "description": "Filter by county (partial match, case-insensitive)",
            "schema": {
              "type": "string",
              "example": "臺北市"
            }
          },
          {
            "name": "municipality",
            "in": "query",
            "required": false,
            "description": "Filter by municipality (partial match, case-insensitive)",
            "schema": {
              "type": "string",
              "example": "大安區"
            }
          },
          {
            "name": "locality",
            "in": "query",
            "required": false,
            "description": "Filter by locality (partial match, case-insensitive)",
            "schema": {
              "type": "string",
              "example": "公園"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "description": "Maximum number of records to return",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "example": 50
            }
          },
          {
            "name": "offset",
            "in": "query",
            "required": false,
            "description": "Number of records to skip (for pagination)",
            "schema": {
              "type": "integer",
              "minimum": 0,
              "default": 0,
              "example": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved TBIA data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/TBIADataRecord"
                      }
                    },
                    "pagination": {
                      "$ref": "#/components/schemas/PaginationInfo"
                    },
                    "filters": {
                      "$ref": "#/components/schemas/TBIAFilters"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - invalid parameters",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LightDataRecord": {
        "type": "object",
        "properties": {
          "time": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp of the light measurement"
          },
          "longitude": {
            "type": "number",
            "format": "double",
            "description": "Longitude coordinate"
          },
          "latitude": {
            "type": "number",
            "format": "double",
            "description": "Latitude coordinate"
          },
          "brightness": {
            "type": "number",
            "format": "double",
            "description": "Light brightness measurement"
          }
        },
        "required": ["time", "longitude", "latitude", "brightness"]
      },
      "TBIADataRecord": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "Unique record identifier"
          },
          "source_scientific_name": {
            "type": "string",
            "nullable": true,
            "description": "Original scientific name from source"
          },
          "scientific_name": {
            "type": "string",
            "nullable": true,
            "description": "Standardized scientific name"
          },
          "common_name_c": {
            "type": "string",
            "nullable": true,
            "description": "Common name in Chinese"
          },
          "bio_group": {
            "type": "string",
            "nullable": true,
            "description": "Biological group classification",
            "enum": ["鳥類", "兩棲類", "哺乳類", "爬蟲類", "魚類", "昆蟲", "蜘蛛"]
          },
          "event_date": {
            "type": "string",
            "format": "date-time",
            "nullable": true,
            "description": "Date when the observation occurred"
          },
          "created": {
            "type": "string",
            "format": "date-time",
            "nullable": true,
            "description": "Date when the record was created"
          },
          "dataset_name": {
            "type": "string",
            "nullable": true,
            "description": "Name of the source dataset"
          },
          "basis_of_record": {
            "type": "string",
            "nullable": true,
            "description": "Type of record (e.g., Occurrence)"
          },
          "standard_latitude": {
            "type": "number",
            "format": "double",
            "nullable": true,
            "description": "Standardized latitude coordinate"
          },
          "standard_longitude": {
            "type": "number",
            "format": "double",
            "nullable": true,
            "description": "Standardized longitude coordinate"
          },
          "county": {
            "type": "string",
            "nullable": true,
            "description": "County where observation was made"
          },
          "municipality": {
            "type": "string",
            "nullable": true,
            "description": "Municipality where observation was made"
          },
          "locality": {
            "type": "string",
            "nullable": true,
            "description": "Specific locality of observation"
          },
          "organism_quantity": {
            "type": "string",
            "nullable": true,
            "description": "Number of organisms observed"
          },
          "taxon_id": {
            "type": "string",
            "nullable": true,
            "description": "Taxonomic identifier"
          },
          "catalog_number": {
            "type": "string",
            "nullable": true,
            "description": "Catalog number in collection"
          },
          "record_number": {
            "type": "string",
            "nullable": true,
            "description": "Record number"
          }
        },
        "required": ["id"]
      },
      "PaginationInfo": {
        "type": "object",
        "properties": {
          "total": {
            "type": "integer",
            "description": "Total number of records matching the query"
          },
          "limit": {
            "type": "integer",
            "nullable": true,
            "description": "Maximum number of records returned"
          },
          "offset": {
            "type": "integer",
            "description": "Number of records skipped"
          },
          "has_more": {
            "type": "boolean",
            "description": "Whether there are more records available"
          }
        },
        "required": ["total", "offset", "has_more"]
      },
      "TimeRange": {
        "type": "object",
        "properties": {
          "start": {
            "type": "string",
            "format": "date-time",
            "description": "Start time of the query range"
          },
          "end": {
            "type": "string",
            "format": "date-time",
            "description": "End time of the query range"
          }
        },
        "required": ["start", "end"]
      },
      "TBIAFilters": {
        "type": "object",
        "properties": {
          "time_range": {
            "$ref": "#/components/schemas/TimeRange"
          },
          "bio_group": {
            "type": "string",
            "nullable": true,
            "description": "Applied bio group filter"
          },
          "common_name_c": {
            "type": "string",
            "nullable": true,
            "description": "Applied common name filter"
          },
          "county": {
            "type": "string",
            "nullable": true,
            "description": "Applied county filter"
          },
          "municipality": {
            "type": "string",
            "nullable": true,
            "description": "Applied municipality filter"
          },
          "locality": {
            "type": "string",
            "nullable": true,
            "description": "Applied locality filter"
          }
        },
        "required": ["time_range"]
      },
      "CountyAnimalData": {
        "type": "object",
        "properties": {
          "county": {
            "type": "string",
            "description": "County name"
          },
          "animals": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "animal_type": {
                  "type": "string",
                  "description": "Type of animal"
                },
                "total_amount": {
                  "type": "integer",
                  "description": "Total amount of animals observed"
                },
                "event_count": {
                  "type": "integer",
                  "description": "Number of observation events"
                }
              },
              "required": ["animal_type", "total_amount", "event_count"]
            }
          }
        },
        "required": ["county", "animals"]
      },
      "CountyTotalData": {
        "type": "object",
        "properties": {
          "county": {
            "type": "string",
            "description": "County name"
          },
          "total_amount": {
            "type": "integer",
            "description": "Total amount of animals observed"
          }
        },
        "required": ["county", "total_amount"]
      },
      "DatasetStatsData": {
        "type": "object",
        "properties": {
          "dataset": {
            "type": "string",
            "description": "Dataset name"
          },
          "count": {
            "type": "integer",
            "description": "Number of records in the dataset"
          }
        },
        "required": ["dataset", "count"]
      },
      "MonthlyData": {
        "type": "object",
        "properties": {
          "month": {
            "type": "integer",
            "minimum": 1,
            "maximum": 12,
            "description": "Month number (1-12)"
          },
          "event_count": {
            "type": "integer",
            "description": "Number of events in the month"
          }
        },
        "required": ["month", "event_count"]
      },
      "CountyLightData": {
        "type": "object",
        "properties": {
          "county": {
            "type": "string",
            "description": "County name"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/MonthlyLightData"
            }
          }
        },
        "required": ["county", "data"],
        "example": {
          "county": "新北市",
          "data": [
            {
              "month": 1,
              "light_pollution_average": 6.70
            },
            {
              "month": 2,
              "light_pollution_average": 6.85
            },
            {
              "month": 3,
              "light_pollution_average": 6.21
            }
          ]
        }
      },
      "MonthlyLightData": {
        "type": "object",
        "properties": {
          "month": {
            "type": "integer",
            "minimum": 1,
            "maximum": 12,
            "description": "Month number (1-12)"
          },
          "light_pollution_average": {
            "type": "number",
            "format": "double",
            "description": "Average light pollution value for the month"
          }
        },
        "required": ["month", "light_pollution_average"],
        "example": {
          "month": 6,
          "light_pollution_average": 3.96
        }
      },
      "AreaLightData": {
        "type": "object",
        "properties": {
          "area": {
            "type": "string",
            "description": "Taiwan region name",
            "enum": ["北區", "中區", "南區", "東區"]
          },
          "light_pollution_average": {
            "type": "number",
            "format": "double",
            "description": "Average light pollution value for the region"
          }
        },
        "required": ["area", "light_pollution_average"],
        "example": {
          "area": "北區",
          "light_pollution_average": 13.65
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "description": "Error message"
          }
        },
        "required": ["error"]
      }
    }
  },
